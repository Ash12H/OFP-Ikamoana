        <!-- ============================================================== -->
        <shallow_sea_to_ocean>False</shallow_sea_to_ocean>
        <landmask_from_habitat>True</landmask_from_habitat>
    </taxis>

    <diffusion>
        <sigma>0.1769952864978924</sigma>
        <diffusion_boost>0</diffusion_boost>
        <diffusion_scale>1</diffusion_scale>
        <sig_scale>1</sig_scale>
        <c_scale>1</c_scale>
        <c>0.662573993401526</c>
        <P>3</P>
    </diffusion>
    
