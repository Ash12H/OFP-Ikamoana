<?xml version="1.0" encoding="UTF-8" ?>

<fields>

    <limit use="False">
        <time_start></time_start>
        <time_end></time_end>
        <!-- North/South -->
        <lat_min></lat_min>
        <lat_max></lat_max>
        <!-- West/Est -->
        <lon_min></lon_min>
        <lon_max></lon_max>
    </limit>

    <general>
        <units>m_per_s</units>
        <!-- <timestep>30*24*60*60</timestep> -->
        <timestep>2592000</timestep>
