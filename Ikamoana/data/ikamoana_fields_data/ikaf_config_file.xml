<?xml version="1.0" encoding="UTF-8" ?>

<fields>

    <limit use="False">
        <time_start></time_start>
        <time_end></time_end>
        <!-- North/South -->
        <lat_min></lat_min>
        <lat_max></lat_max>
        <!-- West/Est -->
        <lon_min></lon_min>
        <lon_max></lon_max>
    </limit>

    <general>
        <units>m_per_s</units>
        <!-- <timestep>30*24*60*60</timestep> -->
        <timestep>2592000</timestep>
    </general>

    <taxis>
        <taxis_scale>1</taxis_scale>
        <shallow_sea_to_ocean>False</shallow_sea_to_ocean>
        <landmask_from_habitat>True</landmask_from_habitat>

        <!-- TODO : These parameters should be found in FeedingHabitat config file -->
        <!-- <Nom> : <vmax_a>2.225841100458143</vmax_a> -->
        <!-- <Nom> : <vmax_b>0.8348850216641774</vmax_b> -->
        <!-- ============================================================== -->
    </taxis>

    <diffusion>
        <diffusion_boost>0</diffusion_boost>
        <diffusion_scale>1</diffusion_scale>

        <!-- TODO : These parameters should be found in FeedingHabitat config file -->
        <!-- <Nom> : <sigma>0.1769952864978924</sigma> -->
        <!-- <Nom> : <c>0.662573993401526</c> -->
        <!-- ============================================================== -->

        <c_scale>1</c_scale>
        <sig_scale>1</sig_scale>

        <!-- HARD CODED ? -->
        <P>3</P>
    </diffusion>

    <!-- TODO : strfile_u et strfile_v dans SEAPODYM XML config file -->
    <current>
        <filepath></filepath>
    </current>
    <!-- ============================================================== -->
    
</fields>